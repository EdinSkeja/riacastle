.commanddef

	h3 Presentation

	dl.group
		dt ID
		dd #{id}
		dt Reward
		dd #{whocoin}
		if typeof targetcoin !== "undefined"
			dt Target reward
			dd #{targetcoin}
		dt Description
		dd #{description}


	if typeof args !== "undefined"
		h3 Arguments
		dl.group
			-for(var t in args)
				dt #{t}
				dd #{args[t]}